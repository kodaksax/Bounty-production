{
  "expo": {
    "name": "BOUNTY",
    "slug": "BOUNTYExpo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "bountyexpo-workspace",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.bounty0.BOUNTYExpo",
        "infoPlist": {
          "ITSAppUsesNonExemptEncryption": false,
          "NSPhotoLibraryUsageDescription": "BOUNTY needs access to your photo library to let you choose a profile picture and add portfolio media.",
          "NSPhotoLibraryAddUsageDescription": "BOUNTY saves selected images to your library when needed."
        }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "@stripe/stripe-react-native",
        {
          "merchantIdentifier": "merchant.com.bountyexpo-workspace"
        }
      ],
      ["@react-native-google-signin/google-signin"],
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-secure-store",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "b5485f88-0b1f-4622-bbed-b1ae142dcb46"
      }
    }
  }
}
