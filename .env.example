# BOUNTYExpo Environment Variables Configuration
# Copy this file to .env and fill in your actual values

# Stripe Configuration (CRITICAL - Replace with your keys)
EXPO_PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_live_your_actual_stripe_key_here
# WARNING: Never commit actual live keys to version control

# API Configuration
EXPO_PUBLIC_API_BASE_URL=https://your-hostinger-domain.com
EXPO_PUBLIC_AUTH_API_URL=https://your-hostinger-api.com

# Database Configuration (if needed for client)
# DATABASE_URL=postgresql://username:password@host:port/database

# JWT Configuration (if handling client-side)
# JWT_SECRET=your_jwt_secret_here

# App Configuration
EXPO_PUBLIC_APP_ENV=development
EXPO_PUBLIC_APP_VERSION=1.0.0

# Stripe Merchant Configuration
EXPO_PUBLIC_STRIPE_MERCHANT_ID=merchant.com.bountyexpo-workspace